package paquete1;

public class General {
	String nombre;
	int Pin;
	int noCuenta;
	float Money;
	
	public General(String nombre, int Pin, int noCuenta, float Money) {
		this.nombre = nombre;
		this.Pin = Pin;
		this.noCuenta = noCuenta;
		this.Money = Money;
	}
	
	public void setName(String nombre) {
		this.nombre = nombre;
	}
	
	public String getName() {
		return nombre;
	}

}
